# ML Model Configuration
# Copy this file to .env and update values

# Server Configuration
ML_PORT=5001
DEBUG=False
PYTHONUNBUFFERED=True

# Flask Configuration
FLASK_ENV=production
FLASK_APP=app.py

# API Configuration
API_TIMEOUT=30
MAX_WORKERS=2

# Model Configuration
MODELS_DIR=./models
PREPROCESSOR_CACHE=True

# External Services
BACKEND_API_URL=http://localhost:5000
SATELLITE_API_KEY=your_api_key_here

# Logging
LOG_LEVEL=INFO
LOG_FILE=./logs/ml_server.log
